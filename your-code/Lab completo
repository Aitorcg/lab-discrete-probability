     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "#e=2,71828\n",
    "#e**-2,3=0.10026\n",
    "average=2.3\n",
    "\n",
    "\n",
    "Probabilidad_5_goles=(((average**5)/math.factorial(5))*(0.10026))\n",
    "Probabilidad_5_goles"
   ]
